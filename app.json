{
  "expo": {
    "name": "Pinky Pocket",
    "slug": "pinky-pocket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D1B3D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pocketpinky.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2D1B3D"
      },
      "package": "com.pocketpinky.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Pinky to access your photos to set your profile picture."
        }
      ]
    ]
  }
}